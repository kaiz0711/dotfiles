// Just some comment
{
    "Lua.diagnostics.disable": [
        "close-non-object",
    ],
    "Lua.runtime.version": "Lua 5.4",
    // Just some comment
    "Lua.workspace.maxPreload": 1600,
    "Lua.workspace.preloadFileSize": 1000,
    "Lua.diagnostics.neededFileStatus": {
        "undefined-field": "Any"
    },
    "Lua.workspace.ignoreDir": [
        "locale/",
        "libs/",
        "3rd",
        ".vscode",
        "meta",
    ],
    "Lua.workspace.library": [
        // Just some comment
        "script/meta",
        "bin",
    ],
    "Lua.runtime.path": [
        "?.lua",
        "script/?.lua",
        "script/?/init.lua"
    ],
    "Lua.misc.parameters": [
        "--develop=true",
        "--dbgport=11413",
    ]
}
